alias u eshell/sudo $*
alias md mkdir $*
alias la ls -la $*
alias install eshell/sudo pacman -Syu $*
alias take mkdir $* && cd $*
alias rf rm -rf $*
alias d cd $* && ls -la
alias menv python3 -m venv ./.venv
alias senv source .venv/bin/activate
alias denv deactivate
alias start eshell/sudo systemctl start $*
alias art eshell/sudo systemctl start $*
alias stop eshell/sudo systemctl stop $*
alias op eshell/sudo systemctl stop $*
alias restart eshell/sudo systemctl restart $*
alias rart eshell/sudo systemctl restart $*
alias status eshell/sudo systemctl status $*
alias us eshell/sudo systemctl status $*
alias enable eshell/sudo systemctl enable $*
alias en eshell/sudo systemctl enable $*
alias disable eshell/sudo systemctl disable $*
alias is eshell/sudo systemctl disable $*
alias enart eshell/sudo systemctl enable --now $*
alias tree exa -T
alias sst eshell/sudo systemctl start $*
alias ssp eshell/sudo systemctl stop $*
alias srt eshell/sudo systemctl restart $*
alias sss eshell/sudo systemctl status $*
alias see eshell/sudo systemctl enable $*
alias seen eshell/sudo systemctl enable --now $*
alias sde eshell/sudo systemctl disable $*