alias kill /usr/bin/kill $*
alias grep /usr/bin/grep $*
alias sdl systemd-analyze verify $*
alias clear eshell-clear-buffer
alias grrc git rm -r --cached $*
alias grc git rm --cached $*
alias gra git remote add $*
alias gpu git push -u $*
alias gp git push $*
alias gc git commit $*
alias ga git add $*
alias sde eshell/sudo systemctl disable $*
alias seen eshell/sudo systemctl enable --now $*
alias see eshell/sudo systemctl enable $*
alias sss eshell/sudo systemctl status $*
alias srt eshell/sudo systemctl restart $*
alias ssp eshell/sudo systemctl stop $*
alias sst eshell/sudo systemctl start $*
alias tree exa -T
alias denv deactivate
alias menv python3 -m venv ./.venv
alias E cd ~/.source
alias S cd ~/.source
alias D cd ~/Downloads
alias dot cd ~/.dotfiles
alias d cd $* && ls -la
alias rf rm -rf $*
alias take mkdir $* && cd $*
alias install eshell/sudo pacman -Syu $*
alias remove eshell/sudo sudo pacman -R $*
alias la ls -la $*
alias ula /usr/bin/ls -la $*
alias u eshell/sudo $*
alias activate-venv my-eshell-activate-venv
alias md mkdir $*
